CAAA,CAAA,CAAA,CAPA,EAQA,IARA,CAQA,QARA,EAQA,MARA,EAnCA,CA6CA;;AACA,IAAA,GAAA,CAAA,SAAA,CAAA,UAAA,EACA,IADA,CACA,KAAA,SADA,EAEA,KAFA,GAGA,MAHA,CAGA,QAHA,EAIA,IAJA,CAIA,IAJA,EAIA,UAAA,CAAA,EAAA;AAAA,aAAA,CAAA,CAAA,CAAA,CAAA,KAAA,CAAA;AAAA,KAJA,EAKA,IALA,CAKA,IALA,EAKA,UAAA,CAAA,EAAA;AAAA,aAAA,CAAA,CAAA,CAAA,CAAA,KAAA,CAAA;AAAA,KALA,EAMA,IANA,CAMA,GANA,EAMA,GANA,EAOA,KAPA,CAOA,MAPA,EAOA,UAAA,CAAA,EAAA;AAAA,aAAA,CAAA,CAAA,KAAA;AAAA,KAPA,EAQA,IARA,CAQA,QARA,EAQA,OARA;AASA;AAxEA,CAAA","sourcesContent":["<template>\n<div id=\"test\">\n     <button @click=\"changeColor('blue')\">Get blue</button>\n<button @click=\"changeColor('red')\">Get red</button>\n\n</div>\n</template>\n<script>\nimport * as d3 from 'd3'\nexport default {\n  props: [\n    'inputData'\n\n  ],\n  data () {\n    return {\n    }\n  },\n  methods: {\n    changeColor (color) {\n      d3.selectAll('circle')\n        .transition()\n        .duration(2000)\n        .style('fill', color)\n    }\n  },\n  mounted: function () {\n    // set the dimensions and margins of the graph\n    const margin = { top: 30, right: 30, bottom: 70, left: 60 }\n    const width = 550 - margin.left - margin.right\n    const height = 520 - margin.top - margin.bottom\n\n    // append the svg object to the body of the page\n    var svg = d3.select('#test')\n      .append('svg')\n      .attr('width', width + margin.left + margin.right)\n      .attr('height', height + margin.top + margin.bottom)\n      .append('g')\n      .attr('transform',\n        'translate(' + margin.left + ',' + margin.top + ')')\n    var x = d3.scaleBand()\n      .range([0, width])\n      .domain(this.inputData.map(function (d) { return d.label }))\n      .padding(1)\n    svg.append('g')\n      .attr('transform', 'translate(0,' + height + ')')\n      .call(d3.axisBottom(x))\n      .selectAll('text')\n      .attr('transform', 'translate(-10,0)rotate(-45)')\n      .style('text-anchor', 'end')\n      .style('font-size', 15)\n\n    // Add Y axis\n    const max = Math.max.apply(null, this.inputData.map(item => item.value))\n    var y = d3.scaleLinear()\n      .domain([0, max])\n      .range([height, 0])\n    svg.append('g')\n      .call(d3.axisLeft(y))\n      .selectAll('text')\n      .style('font-size', 15)\n    svg.selectAll('myline')\n      .data(this.inputData)\n      .enter()\n      .append('line')\n      .attr('x1', function (d) { return x(d.label) })\n      .attr('x2', function (d) { return x(d.label) })\n      .attr('y1', function (d) { return y(d.value) })\n      .attr('y2', y(0))\n      .attr('stroke', 'grey')\n\n    // Circles\n    svg.selectAll('mycircle')\n      .data(this.inputData)\n      .enter()\n      .append('circle')\n      .attr('cx', function (d) { return x(d.label) })\n      .attr('cy', function (d) { return y(d.value) })\n      .attr('r', '9')\n      .style('fill', function (d) { return d.color })\n      .attr('stroke', 'black')\n  }\n}\n\n</script>\n"],"sourceRoot":"src/components"}]}